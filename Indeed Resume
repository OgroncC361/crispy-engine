@keyframes scaleIn { 
  0% { opacity: 0; transform: scale3d(0.3, 0.3, 0.3); }
  50% { opacity: 1; }
}

@keyframes fadeIn { 
  0% { opacity: 0; }
  100% { opacity: 1; }
}

@keyframes fadeInUp { 
  0% { opacity: 0; transform: translate3d(0px, 100%, 0px); }
  100% { opacity: 1; transform: translateZ(0px); }
}

@keyframes fadeInRight-ltr { 
  0% { opacity: 0; transform: translate3d(100%, 0px, 0px); }
  100% { opacity: 1; transform: translateZ(0px); }
}

@keyframes fadeInRight-rtl { 
  0% { opacity: 0; transform: translate3d(-100%, 0px, 0px); }
  100% { opacity: 1; transform: translateZ(0px); }
}

@keyframes fadeInDown { 
  0% { opacity: 0; transform: translate3d(0px, -100%, 0px); }
  100% { opacity: 1; transform: translateZ(0px); }
}

@keyframes fadeInLeft-ltr { 
  0% { opacity: 0; transform: translate3d(-100%, 0px, 0px); }
  100% { opacity: 1; transform: translateZ(0px); }
}

@keyframes fadeInLeft-rtl { 
  0% { opacity: 0; transform: translate3d(100%, 0px, 0px); }
  100% { opacity: 1; transform: translateZ(0px); }
}

@keyframes scaleOut { 
  0% { opacity: 1; }
  50% { opacity: 0; transform: scale3d(0.3, 0.3, 0.3); }
  100% { opacity: 0; }
}

@keyframes fadeOut { 
  0% { opacity: 1; }
  100% { opacity: 0; }
}

@keyframes fadeOutUp { 
  0% { opacity: 1; }
  100% { opacity: 0; transform: translate3d(0px, -100%, 0px); }
}

@keyframes fadeOutRight-ltr { 
  0% { opacity: 1; }
  100% { opacity: 0; transform: translate3d(100%, 0px, 0px); }
}

@keyframes fadeOutRight-rtl { 
  0% { opacity: 1; }
  100% { opacity: 0; transform: translate3d(-100%, 0px, 0px); }
}

@keyframes fadeOutDown { 
  0% { opacity: 1; }
  100% { opacity: 0; transform: translate3d(0px, 100%, 0px); }
}

@keyframes fadeOutLeft-ltr { 
  0% { opacity: 1; }
  100% { opacity: 0; transform: translate3d(-100%, 0px, 0px); }
}

@keyframes fadeOutLeft-rtl { 
  0% { opacity: 1; }
  100% { opacity: 0; transform: translate3d(100%, 0px, 0px); }
}

@keyframes rotate-ltr { 
  0% { transform: rotate(0deg); }
  100% { transform: rotate(1turn); }
}

@keyframes rotate-rtl { 
  0% { transform: rotate(0deg); }
  100% { transform: rotate(-1turn); }
}

@keyframes flipX-ltr { 
  0% { transform: rotateX(90deg); }
  100% { transform: rotateX(0deg); }
}

@keyframes flipX-rtl { 
  0% { transform: rotateX(-90deg); }
  100% { transform: rotateX(0deg); }
}

